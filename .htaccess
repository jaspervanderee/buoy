# ==== Buoy canonical redirects (static compare/service) ====
RewriteEngine On


# BEGIN buoy compare redirects
RewriteRule ^unchained-vs-anchorwatch\.html$ /anchorwatch-vs-unchained.html [R=301,L]
RewriteRule ^breez-vs-aqua\.html$ /aqua-vs-breez.html [R=301,L]
RewriteRule ^muun-vs-aqua\.html$ /aqua-vs-muun.html [R=301,L]
RewriteRule ^phoenix-vs-aqua\.html$ /aqua-vs-phoenix.html [R=301,L]
RewriteRule ^muun-vs-breez\.html$ /breez-vs-muun.html [R=301,L]
RewriteRule ^phoenix-vs-breez\.html$ /breez-vs-phoenix.html [R=301,L]
RewriteRule ^phoenix-vs-muun\.html$ /muun-vs-phoenix.html [R=301,L]
RewriteRule ^relai-vs-bitonic\.html$ /bitonic-vs-relai.html [R=301,L]
RewriteRule ^river-vs-bitonic\.html$ /bitonic-vs-river.html [R=301,L]
RewriteRule ^strike-vs-bitonic\.html$ /bitonic-vs-strike.html [R=301,L]
RewriteRule ^swan-vs-bitonic\.html$ /bitonic-vs-swan.html [R=301,L]
RewriteRule ^river-vs-relai\.html$ /relai-vs-river.html [R=301,L]
RewriteRule ^strike-vs-relai\.html$ /relai-vs-strike.html [R=301,L]
RewriteRule ^swan-vs-relai\.html$ /relai-vs-swan.html [R=301,L]
RewriteRule ^strike-vs-river\.html$ /river-vs-strike.html [R=301,L]
RewriteRule ^swan-vs-river\.html$ /river-vs-swan.html [R=301,L]
RewriteRule ^swan-vs-strike\.html$ /strike-vs-swan.html [R=301,L]
RewriteRule ^peach-vs-hodlhodl\.html$ /hodlhodl-vs-peach.html [R=301,L]
RewriteRule ^pocket-vs-hodlhodl\.html$ /hodlhodl-vs-pocket.html [R=301,L]
RewriteRule ^pocket-vs-peach\.html$ /peach-vs-pocket.html [R=301,L]
RewriteRule ^start9-vs-mynode\.html$ /mynode-vs-start9.html [R=301,L]
RewriteRule ^umbrel-vs-mynode\.html$ /mynode-vs-umbrel.html [R=301,L]
RewriteRule ^umbrel-vs-start9\.html$ /start9-vs-umbrel.html [R=301,L]
RewriteRule ^wasabi-vs-sparrow\.html$ /sparrow-vs-wasabi.html [R=301,L]
# END buoy compare redirects

# Fallback for any X-vs-Y page â†’ dynamic compare (only if file doesn't exist)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^([a-z0-9-]+)-vs-([a-z0-9-]+)\.html$ /compare.html?services=$1,$2 [QSA,R=301,L]


